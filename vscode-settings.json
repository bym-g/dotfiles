{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.parameterHints.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontFamily": "Ubuntu Mono",
  "editor.fontSize": 16,
  "terminal.integrated.fontFamily": "Ubuntu Mono",
  "terminal.integrated.fontSize": 16,
  "editor.formatOnSave": true,
  "explorer.openEditors.visible": 0,
  "window.zoomLevel": -1,
  "bracket-pair-colorizer-2.colors": ["#8FB5FE", "#FFFFFF", "#91DC47"],
  "bracket-pair-colorizer-2.unmatchedScopeColor": "#FF5555",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#bd93f9",
  "vim.statusBarColors.insert": "#00FF9C",
  "vim.statusBarColors.visual": "#ffb86c",
  "vim.statusBarColors.visualblock": "#ffb86c",
  "vim.statusBarColors.visualline": "#ffb86c",
  "vim.statusBarColors.replace": "#ff5555",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-e>"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.revealDefinition"]
    }
  ],
  "vim.visualModeKeyBindings": [],
  "editor.lineNumbers": "relative"
}
