{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Dracula Soft",
  "workbench.startupEditor": "none",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.parameterHints.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontFamily": "Ubuntu Mono",
  "editor.fontSize": 18,
  "terminal.integrated.fontFamily": "Ubuntu Mono",
  "terminal.integrated.fontSize": 18,
  "editor.formatOnSave": true,
  "explorer.openEditors.visible": 0,
  "window.zoomLevel": -1,
  "workbench.panel.defaultLocation": "right",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#44475a",
  "vim.statusBarColors.insert": "#6272a4",
  "vim.statusBarColors.visual": "#B48EAD",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "vim.easymotion": true,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<space>"
      ],
      "after": [
        "leader",
        "leader",
        "leader",
        "b",
        "d",
        "w"
      ]
    }
  ],
  "vim.easymotionMarkerFontFamily": "Ubuntu Mono",
  "vim.easymotionMarkerYOffset": 10,
  "vim.easymotionMarkerFontSize": "20",
  "vim.easymotionMarkerHeight": 24,
  "vim.easymotionMarkerWidthPerChar": 12,
  "workbench.colorCustomizations": {
    "statusBar.background": "#B48EAD",
    "statusBar.noFolderBackground": "#B48EAD",
    "statusBar.debuggingBackground": "#B48EAD"
  }
}